= '0rem';\n\t\t\t} else {\n\t\t\t\tevent.target.style.backgroundColor = '';\n\t\t\t\tevent.target.style.borderBottom = \"1px solid #000000\";\n\t\t\t\tevent.target.previousElementSibling.style.top = '';\n\t\t\t\tevent.target.previousElementSibling.style.fontSize = '';\n\t\t\t\tevent.target.previousElementSibling.style.marginTop = '0.3rem';\n\t\t\t}\n\t\t},\n\t\tonlyNumbers: function() {\n\t\tvar regex = /[^0-9]/gi;\n\t\tthis.user.phone = this.user.phone.replace(regex,'');\n\t\t},\n\t\t// onlyText: function() {\n\t\t//   var regex = /[^0-9]/gi;\n\t\t//   this.user.phone = this.user.phone.replace(regex,'');\n\t\t// },user.emailVerified\n\t\tonSignup () {\n\t\t\tthis.loading = true\n\t\t\tthis.$store.dispatch('signUp',{\n\t\t\t\temail: this.user.email,\n\t\t\t\tpassword: this.user.password,\n\t\t\t\tname: this.user.name,\n\t\t\t\tphone: this.user.phone,\n\t\t\t\tcreated_at: this.user.created_at,\n\t\t\t\tstatus: this.user.status,\n\t\t\t})\n\t\t\t.then( res => {\n\t\t\t\tif (res.emailVerified) {\n\t\t\t\t\tthis.loading = false;\n\t\t\t\t\tthis.message = 'Your sign up was successful, awaiting platform approval.';\n\t\t\t\t\tthis.$router.push('/account');\n\t\t\t\t}\n\t\t\t\telse {\n\t\t\t\t\tthis.loading = false;\n\t\t\t\t\tthis.message = `An email verification link has been sent to ${res.email}\\nKindly verify your account to countinue using the platform.`;\n\t\t\t\t\tthis.$emit('message', this.message)\n\t\t\t\t\tthis.$router.push('/account');\n\t\t\t\t}\n\t\t\t})\n\t\t\t.catch(err => {\n\t\t\t\tthis.loading = false;\n\t\t\t\tthis.message = err.message;\n\t\t\t\tthis.$emit('message', this.message)\n\t\t\t\tthis.$router.push('/account');\n\t\t\t\treturn err;\n\t\t\t})\n\t\t}\n\t}\n}\n</script>\n\n<style scoped>\n.signup {\n\tbackground-color: #FFFFFF;\n}\n.title {\n\ttext-align: center;\n\tfont-weight: 500;\n}\n.form-item {\n\tdisplay: inline-block;\n\tpadding: 1rem;\n\tmargin-bottom: 1rem;\n\t/* border: 1px solid #555; */\n\tborder-radius: 5px;\n\twidth: 100%;\n\tposi